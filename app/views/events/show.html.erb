<%= form_for(Reservation.new) do |f| %>
  <div><%= hidden_field_tag :event_id, @event.id %></div>
  <%= f.submit "Reserve", class: "btn btn-primary" %>
<% end %>

<%= link_to 'Edit', edit_event_path(@event) %>
<%= link_to 'Back', events_path %>
