<p id="notice"><%= notice %></p>

<p>
  <strong>Start:</strong>
  <br />
  <%= @event.id %>
  <br />
  <%= @event.start %>
  <br />
  <%= @event.user.username %>
</p>
<!--
<p>
  <strong>End:</strong>
  <%= @event.end %>
</p>
